{
  presets: [
    '@babel/preset-stage-0',
    ['@babel/preset-env',
      { modules: 'commonjs'
      }
    ],
    '@babel/preset-flow',
    'next/babel',
  ],
  plugins: [
    [
      'import',
      {
        libraryName: 'antd',
      },
    ],
    ['styled-components',
      { ssr: true, displayName: true, preprocess: false
      }
    ],
  ],
}